from tkinter import *

top=Tk()
top.title("조선대학교 108계단 오르기")
top.geometry("440x810")

label1=Label(top, text="조선대학교 108계단 오르기", font="Time 10")
label1.place(x=138, y=50)

label2=Label(top, text="최고점수")
label2.place(x=195, y=300)

f=open("scoreboard.txt", 'r')
label3=Label(top, text=f.read())
label3.place(x=210,y=320)

btn1=Button(top,text="게임시작")
btn1.place(x=380, y=783)

